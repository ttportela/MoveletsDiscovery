{
   "input": {
          "train": ["geo_only_train"],
          "test": ["geo_only_test"],
          "format": "CSV",
          "loader": "interning"
   },
   "idFeature": {
          "order": 1,
          "type": "numeric",
          "text": "tid"
    },
   "labelFeature": {
          "order": 2,
          "type": "nominal",
          "text": "label"
    },
   "attributes": [
	    {
	          "order": 1,
	          "type": "space2d",
	          "text": "lat_lon",
	          "comparator": {
	            "distance": "euclidean",
	            "maxValue": -1
	          }
	    }
	]
}
